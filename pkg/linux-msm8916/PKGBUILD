# Maintainer: Sam Day <me@samcday.com>
pkgname=linux-msm8916
pkgver=6.6
pkgrel=2
pkgdesc="pmOS linux-postmarketos-qcom-msm8916 repackaged for Arch"
arch=('aarch64')
url="https://www.gnu.org/software/patch/patch.html"
license=('GPL')
options=('!strip')
source=("http://mirror.postmarketos.org/postmarketos/master/aarch64/linux-postmarketos-qcom-msm8916-${pkgver}-r${pkgrel}.apk")
sha256sums=('ad265c562498653f0c34f61771254c613e0ec1285a9dc674bd983ff02d39a841')

package() {
    cp -R $srcdir/boot $pkgdir/
    cp -R $srcdir/usr $pkgdir/
    cp -R $srcdir/lib $pkgdir/usr
    mv $pkgdir/boot/vmlinuz $pkgdir/usr/lib/modules/*-msm8916/
}
